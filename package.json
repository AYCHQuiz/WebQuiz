{
  "name": "web-quiz",
  "version": "0.1.0",
  "description": "Mobile-first web quiz",
  "author": "Hans Kirchner <h.kirchner@tum.de>",
  "license": "BSD-3-Clause",
  "scripts": {
    "build": "webpack",
    "buildp": "webpack -p",
    "watch": "webpack --watch",
    "dev": "NODE_ENV=development node server example-quiz",
    "doc": "apidoc -i lib/ -o apidoc/",
    "test": "mocha lib/test --exit",
    "karma": "karma start karma.conf.js --single-run",
    "karma-watch": "karma start karma.conf.js --no-single-run",
    "lint": "eslint ./ && tslint -c tslint.json 'app/**/*.ts'",
    "csslint": "csslint app/*.css",
    "saucelabs": "karma start karma-saucelabs.conf.js",
    "coverage": "istanbul cover _mocha --report lcovonly -- lib/test  --exit",
    "publish_to_coveralls": "cat ./coverage/lcov.info | coveralls",
    "export_stats": "webpack --json > stats.json"
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "compression": "^1.7.1",
    "css-loader": "^0.28.9",
    "express": "^4.17.3",
    "file-loader": "^1.1.6",
    "glob": "^7.1.2",
    "helmet": "^3.10.0",
    "json-loader": "^0.5.7",
    "mathjax-node": "^1.3.0",
    "mkdirp": "^0.5.1",
    "pug": "^2.0.0-rc.4",
    "spectre.css": "^0.5.0",
    "style-loader": "^0.20.1",
    "svgo": "^1.0.4",
    "ts-loader": "^3.4.0",
    "typescript": "^2.7.1",
    "vue": "^2.5.13",
    "vue-i18n": "^7.4.2",
    "vue-loader": "^14.1.1",
    "vue-template-compiler": "^2.5.13",
    "vue-ts-loader": "0.0.3",
    "webpack": "^3.10.0",
    "yamljs": "^0.3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tum-rt/web-quiz.git"
  },
  "devDependencies": {
    "@types/core-js": "^0.9.46",
    "@types/mocha": "^2.2.48",
    "apidoc": "^0.17.6",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "coveralls": "^3.0.0",
    "csslint": "^1.0.5",
    "eslint": "^4.17.0",
    "istanbul": "^0.4.5",
    "karma": "^2.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-firefox-launcher": "^1.1.0",
    "karma-mocha": "^1.3.0",
    "karma-sauce-launcher": "^1.2.0",
    "karma-webpack": "^2.0.9",
    "mocha": "^5.0.0",
    "tslint": "^5.9.1",
    "webpack-dev-middleware": "^2.0.4",
    "webpack-hot-middleware": "^2.21.0"
  },
  "apidoc": {}
}
