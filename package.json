{
  "name": "web-quiz",
  "version": "0.1.0",
  "description": "Mobile-first web quiz",
  "author": "Hans Kirchner <h.kirchner@tum.de>",
  "license": "BSD-3-Clause",
  "scripts": {
    "build": "webpack",
    "buildp": "webpack -p",
    "watch": "webpack --watch",
    "dev": "NODE_ENV=development node server example-quiz",
    "doc": "apidoc -i lib/ -o apidoc/",
    "test": "mocha lib/test --exit",
    "karma": "karma start karma.conf.js --single-run",
    "karma-watch": "karma start karma.conf.js --no-single-run",
    "lint": "eslint ./ && tslint -c tslint.json 'app/**/*.ts'",
    "saucelabs": "karma start karma-saucelabs.conf.js",
    "coverage": "istanbul cover _mocha --report lcovonly -- lib/test  --exit",
    "publish_to_coveralls": "cat ./coverage/lcov.info | coveralls",
    "export_stats": "webpack --json > stats.json"
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "compression": "^1.7.1",
    "css-loader": "^0.28.4",
    "express": "^4.16.1",
    "file-loader": "^1.1.5",
    "glob": "^7.1.2",
    "helmet": "^3.8.2",
    "json-loader": "^0.5.7",
    "mathjax-node": "^1.2.1",
    "mkdirp": "^0.5.1",
    "pug": "^2.0.0-rc.4",
    "spectre.css": "^0.5.0",
    "style-loader": "^0.19.0",
    "svgo": "^0.7.1",
    "ts-loader": "^2.3.7",
    "typescript": "^2.5.3",
    "vue": "^2.4.2",
    "vue-i18n": "^7.3.1",
    "vue-loader": "^13.0.2",
    "vue-template-compiler": "^2.4.2",
    "vue-ts-loader": "0.0.3",
    "webpack": "^3.6.0",
    "yamljs": "^0.3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tum-rt/web-quiz.git"
  },
  "devDependencies": {
    "@types/core-js": "^0.9.43",
    "@types/mocha": "^2.2.43",
    "apidoc": "^0.17.6",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "coveralls": "^3.0.0",
    "eslint": "^4.8.0",
    "istanbul": "^0.4.5",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-mocha": "^1.3.0",
    "karma-sauce-launcher": "^1.2.0",
    "karma-webpack": "^2.0.4",
    "mocha": "^4.0.1",
    "tslint": "^5.7.0",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.19.1"
  },
  "apidoc": {}
}
